{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "debug": true,
        "modules": "auto",
        "useBuiltIns": "usage"
      }
    ], [
      "@babel/preset-react"
    ]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "corejs": 2,
      // "helpers": true,
      // "regenerator": true,
      // "useESModules": false
    }],
    "@babel/plugin-syntax-dynamic-import",
    "lodash",
    "@babel/plugin-proposal-class-properties"
  ]
}
